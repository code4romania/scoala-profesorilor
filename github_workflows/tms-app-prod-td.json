{ 
    "containerDefinitions": [
        {
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ],
            "cpu": 0,
            "environment": [
                {
                    "name": "APP_KEY",
                    "value": "$APP_KEY"
                },
                {
                    "name": "APP_DEBUG",
                    "value": "false"
                },
                {
                    "name": "APP_ENV",
                    "value": "production"
                },
                {
                    "name": "APP_LOG",
                    "value": "daily"
                },
                {
                    "name": "APP_NAME",
                    "value": "Scoala Profesorilor"
                },
                {
                    "name": "APP_URL",
                    "value": "https://wwww.scoalaprofesorilor.ro"
                },
                {
                    "name": "CMS_ACTIVE_THEME",
                    "value": "genuineq-genuineq-scoala-profesorilor"
                },
                {
                    "name": "CMS_ASSET_CACHE",
                    "value": "true"
                },
                {
                    "name": "CMS_ASSET_DEEP_HASHING",
                    "value": "true"
                },
                {
                    "name": "CMS_ASSET_MINIFY",
                    "value": "true"
                },
                {
                    "name": "CMS_BACKEND_FORCE_REMEMBER",
                    "value": "null"
                },
                {
                    "name": "CMS_BACKEND_FORCE_SECURE",
                    "value": "false"
                },
                {
                    "name": "CMS_BACKEND_URI",
                    "value": "backend-scoala-profesorilor"
                },
                {
                    "name": "CMS_CONVERT_LINE_ENDINGS",
                    "value": "true"
                },
                {
                    "name": "CMS_EDGE_UPDATES",
                    "value": "false"
                },
                {
                    "name": "CMS_ENABLE_CSRF",
                    "value": "true"
                },
                {
                    "name": "CMS_LINK_POLICY",
                    "value": "insecure"
                },
                {
                    "name": "CMS_ROUTES_CACHE",
                    "value": "true"
                },
                {
                    "name": "DB_DATABASE",
                    "value": "tms"
                },
                {
                    "name": "DB_HOST",
                    "value": "$DB_HOST"
                },
                {
                    "name": "DB_PASSWORD",
                    "value": "$DB_PASSWORD"
                },
                {
                    "name": "DB_PORT",
                    "value": "3306"
                },
                {
                    "name": "DB_USERNAME",
                    "value": "tms"
                },
                {
                    "name": "SESSION_DRIVER",
                    "value": "database"
                },
                {
                    "name": "SESSION_LIFETIME",
                    "value": "120"
                },
                {
                    "name": "MAIL_HOST",
                    "value": "$MAIL_HOST"
                },
                {
                    "name": "MAIL_DRIVER",
                    "value": "smtp"
                },
                {
                    "name": "MAIL_USERNAME",
                    "value": "$MAIL_USERNAME"
                },
                {
                    "name": "MAIL_NAME",
                    "value": "Scoala Profesorilor"
                },
                {
                    "name": "MAIL_PORT",
                    "value": "$MAIL_PORT"
                },
                {
                    "name": "MAIL_PASSWORD",
                    "value": "$MAIL_PASSWORD"
                },
                {
                    "name": "MAIL_ENCRYPTION",
                    "value": "$MAIL_ENCRYPTION"
                }
            ],
            "mountPoints": [
                {
                    "containerPath": "/var/www/storage/app",
                    "sourceVolume": "tms_prod_persistent_volume"
                }
            ], 
            "memoryReservation": 512,
            "volumesFrom": [],
            "image": "961595780523.dkr.ecr.eu-central-1.amazonaws.com/production:#TAG",
            "essential": true,
            "links": [],
            "name": "app"
        }
    ],
    "family": "application",
    "requiresCompatibilities": [
        "EC2"
    ],
    "volumes": [
        {
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-2640f37e",
                "authorizationConfig": {
                    "iam": "DISABLED"
                },
                "transitEncryption": "DISABLED",
                "rootDirectory": "/"
            },
            "name": "tms_prod_persistent_volume"
        },
        {
            "name": "temp_pv",
            "host": {
                "sourcePath": "/home/ec2-user/pv"
            }
        },
        {
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-2640f37e",
                "authorizationConfig": {
                    "iam": "DISABLED"
                },
                "transitEncryption": "DISABLED",
                "rootDirectory": "/"
            },
            "name": "efs_pv"
        }
    ],
    "placementConstraints": []
}
